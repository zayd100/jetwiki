{
  "version": 2,
  "builds": [
    {
      "src": "Back-end/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front-end/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Front-end/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Back-end/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Front-end/dist/$1"
    }
  ]
}

